{
	"arrowParens": "avoid",
	"bracketSpacing": true,
	"endOfLine": "lf",
	"htmlWhitespaceSensitivity": "css",
	"jsxBracketSameLine": false,
	"jsxSingleQuote": true,
	"quoteProps": "as-needed",
	"printWidth": 80,
	"parser": "typescript",
	"proseWrap": "preserve",
	"requirePragma": false,
	"semi": true,
	"singleQuote": true,
	"stylelintIntegration": "none",
	"tabWidth": 2,
	"trailingComma": "none",
	"eslintIntegration": true,
	"useTabs": false,
	"overrides": [
		{
			"files": "*.json",
			"options": {
				"printWidth": 200
			}
		}
	]
}
