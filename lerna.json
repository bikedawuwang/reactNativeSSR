{
  "npmClient": "npm",
  "packages": [
    "src/*",
    "src/client/web/app/common/components/*",
    "src/client/web/app/containers/*",
    "src/client/shared/*",
    "src/client/shared/components/*",
    "src/client/shared/state/containers/*",
    "src/client/*",
    "src/client/web/*"
  ],
  "command": {
    "bootstrap": {
      "ignore": "__tests__",
      "npmClientArgs": ["--no-package-lock"]
    }
  },
  "version": "independent"
}
